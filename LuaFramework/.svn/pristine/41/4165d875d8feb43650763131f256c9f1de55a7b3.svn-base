-- syntax =  "proto3",

-- packageGPBClass.Message,
-- import"PB_ItemInfo.proto",

 PB_SevenSignReceiveReward_Req  =  {
    type  =  1,--	int32  type ,-- 默认值0-七日活动全部进度奖励+全部签到奖励，-1历史未领取的全部奖励 1-全部签到奖励，2-全部进度奖励，3-单个签到奖励，4-单个进度奖励
    index  =  2,--	int32  index ,--当领取 单个签到奖励 或  单个进度奖励 时 需要该参数 指定总列表中的下标  从0开始
}


 PB_SevenSignReceiveReward_Res  =  {
    type                                          =  1,-- int32  type ,--type
    reward                          =  2,-- repeated PB_ItemInfo ,--领取的奖励-展示
}


