 PB_MailInfo{
    recordId                   =  1,-- int32 recordId ,--邮件记录ID
    type                       =  2,-- int32 type ,--区分邮件类型，有不同的样式
    title                     =  3,-- string title ,--邮件标题
    content                   =  4,-- string content ,--邮件内容
    reward      =  5,-- repeated PB_ItemInfo ,--奖励数组
    isReceive                   =  6,-- bool isReceive ,--是否领取奖励  true已领  false未领
    isRead                      =  7,-- bool isRead ,--是否已读  true已读  false未读
    expireTime                 =  8,-- int32 expireTime ,--邮件领取倒计时
}
