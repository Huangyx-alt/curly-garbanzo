function M.GetAtlasByCardIconName(iconName)
    return M.cardIconName2AtlasMap[iconName]
end

function M.GetCardBgNameByLevel(level)
    return M.cardBgNames[level]
end

function M.GetGrayCardBgName(level)
	level = level or 1
	if level > 4 then
		return "CardDi08"
	elseif level > 3 then
		return "CardDi07"
	else
		return "CardDi06"
	end
end

function M.GetBagBigIconName(smallIconName)
    return M.bagSmallIcon2BigIconMap[smallIconName]
end

--将数字格式化为以“K”单位结尾
function M.FormatReward(digit)
	digit = tonumber(digit)
	local count = digit

	if digit >= 1000000 then
		count = math.floor(digit / 1000)
		return fun.format_money(count) .. "K"
	end

	return fun.format_money(count)
end

return M